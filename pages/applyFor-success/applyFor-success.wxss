.page{
  width: 100%;
  height: 100%;
  background: #f6f6f6;
  position: relative;
  overflow: hidden;
}


.success-box{
  margin: 70rpx 20rpx;
  /* width:720rpx;
  height:844rpx; */
  padding-top: 200rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 12px 6px rgba(51,51,51,0.04);
  border-radius:15rpx; 
  padding-bottom: 70rpx;
}
.success-logo{
  width: 122rpx;
  height: 122rpx;
