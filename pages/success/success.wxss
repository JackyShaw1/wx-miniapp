.page{
  width: 100%;
}
.success-icon{
  width: 100%;
  height: 308rpx;
  text-align: center;
  line-height: 308rpx;
}

.success-icon>image{
  width: 384rpx;
  height: 308rpx;
}

.sucess-info{
  width:100%;
  height: 150rpx;
  text-align: center;
}

.companyName{
  line-height: 50rpx;
  font-size: 34rpx;
  color: #333;
  font-weight: 500;
}

.success-tip{
  font-size: 30rpx;
  line-height: 50rpx;
  color: #333;
}

.preview-btn{
  width: 612rpx;
  height: 91rpx;
  margin: 0 auto;
  line-height: 80rpx;
  font-size: 30rpx;
  text-align: center;
  background: linear-gradient(to right, #d0323f , #fe5029);
  color: #fff;
  border-radius: 8rpx;
}

.preview-btn-hover{
  background: linear-gradient(90deg,rgba(255,90,100,1) 0%,rgba(255,150,160,1) 100%);
  color: #fff;
}